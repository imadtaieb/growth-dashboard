# Supabase Configuration (REQUIRED for database)
# Get these from your Supabase project settings:
# https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here

# Google Sheets Configuration (optional - for public sheets)
# Get an API key from https://console.cloud.google.com
GOOGLE_SHEETS_API_KEY=your_google_api_key_here

# PostHog Configuration (optional)
POSTHOG_API_KEY=phc_your_api_key_here
POSTHOG_PROJECT_ID=your_project_id_here

# PostHog Webhook Security (optional but recommended)
# Generate a random secret token for webhook authentication
POSTHOG_WEBHOOK_SECRET=your_random_secret_token_here

# Cron Job Security (required for scheduled syncing)
# Generate a random secret token for cron endpoint authentication
CRON_SECRET=your_random_cron_secret_here

# Tally Configuration (optional)
TALLY_API_KEY=your_tally_api_key_here

# Slack Configuration (optional)
SLACK_BOT_TOKEN=xoxb-your-token-here
SLACK_CHANNEL_ID=C01234567
SLACK_WEBHOOK_URL=your_slack_webhook_url_here
